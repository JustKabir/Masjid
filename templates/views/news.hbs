        function GetsZiyaratQueueStatus() {
            var XMLObj = false;
            var iconZiyarat_Male = document.getElementById("iconZiyarat_Male");
            var lbZiyarat_Head_Male = document.getElementById("lbZiyarat_Head_Male");
            var lblZiyarat_Date_Male = document.getElementById("lblZiyarat_Date_Male");
            var iconZiyarat_Female = document.getElementById("iconZiyarat_Female");
            var lbZiyarat_Head_Female = document.getElementById("lbZiyarat_Head_Female");
            var lblZiyarat_Date_Female = document.getElementById("lblZiyarat_Date_Female");

            lbZiyarat_Head_Male.innerHTML = "...";
            lblZiyarat_Date_Male.innerHTML = "...";
            lbZiyarat_Head_Female.innerHTML = "...";
            lblZiyarat_Date_Female.innerHTML = "...";

            if (window.XMLHttpRequest) { XMLObj = new XMLHttpRequest(); }
            else if (window.ActiveXObject) { XMLObj = new ActiveXObject("Microsoft.XMLHTTP"); }

            if (XMLObj) {
                XMLObj.open("GET", '/Common/GetDataAjax.aspx?QT=35');
                XMLObj.onreadystatechange = function () {
                    if (XMLObj.readyState == 4 && XMLObj.status == 200 && XMLObj.responseXML != null) {
                        xmlDoc = XMLObj.responseXML;
                        //Male
                        iconZiyarat_Male.style.color = xmlDoc.getElementsByTagName("Line_Status")[0].firstChild.data;
                        lbZiyarat_Head_Male.innerHTML = xmlDoc.getElementsByTagName("Info")[0].firstChild.data;
                        lbZiyarat_Head_Male.style.color = xmlDoc.getElementsByTagName("Line_Status")[0].firstChild.data;

                        //Female
                        iconZiyarat_Female.style.color = xmlDoc.getElementsByTagName("Female_Line_Status")[0].firstChild.data;
                        lbZiyarat_Head_Female.innerHTML = xmlDoc.getElementsByTagName("Female_Line_Info")[0].firstChild.data;
                        lbZiyarat_Head_Female.style.color = xmlDoc.getElementsByTagName("Female_Line_Status")[0].firstChild.data;

                        //Date
                        lblZiyarat_Date_Male.innerHTML = xmlDoc.getElementsByTagName("Modify_Date")[0].firstChild.data;
                        lblZiyarat_Date_Female.innerHTML = xmlDoc.getElementsByTagName("Modify_Date")[0].firstChild.data;
                        jQueryMsg("Ziyarat Queue Status", "Ziyarat Queue Status has been refreshed successfully", 2, 1);
                    }
                }
                XMLObj.send(null);
            }
        }
        function GetMediaLibrary(txt) {
            //http://jqueryui.com/autocomplete/#xml			
            $.ajax({
                url: "/Common/GetDataAjax.aspx?QT=40&Name=" + txt.value,
                dataType: "xml",
                success: function (xmlResponse) {
                    var data = $("Table", xmlResponse).map(function () {
                        return {
                            value: $("media_name", this).text(),
                            link: $("Media_url", this).text(),
                            id: $("id", this).text(),

                        };
                    }).get();

                    $("#txtMediaLibrarySearch").autocomplete({
                        source: data, minLength: 1,
                        select: function (event, ui) { window.open(ui.item.link); },
                    });
                }
            });
        }
