        function ValidatedPassword() {
            document.getElementById("spnerrMsg").innerHTML = "";

            if (document.getElementById("Footer1_txtSessionPwd").value == "") {
                document.getElementById("spnerrMsg").innerHTML = "Enter Password!";
                jQueryMsg('Enter Password!', 'Please enter password');
                return false;
            }

            var xObj = false;
            if (window.XMLHttpRequest)
                xObj = new XMLHttpRequest();
            else if (window.ActiveXObject)
                xObj = new ActiveXObject("Microsoft.XMLHTTP");

            if (xObj) {
                xObj.open("GET", '/GetAjaxSession.aspx?QT=E2DZB&PWD=' + document.getElementById("Footer1_txtSessionPwd").value);
                xObj.onreadystatechange = function () {
                    if (xObj.readyState == 4 && xObj.status == 200 && xObj.responseXML != null) {
                        if (xObj.responseXML.getElementsByTagName("IsValid").length > 0) {
                            if (xObj.responseXML.getElementsByTagName("IsValid")[0].firstChild.data == "true") {
                                document.getElementById("spnerrMsg").innerHTML = "<span class='textgreen'>Logged In Successfully!</span>";
                                jQueryMsg('Success', 'Logged In Successfully!', 2);
                            }
                            else {
                                document.getElementById("spnerrMsg").innerHTML = "Invalid Password!";
                                jQueryMsg('Invalid Password!', 'Entered Invalid Password');
                            }
                        }
                    }
                }
                xObj.send(null);
            }
        }
        function SendOTP() {
            document.getElementById("spnerrMsg").innerHTML = "";

            var xObj = false;
            if (window.XMLHttpRequest)
                xObj = new XMLHttpRequest();
            else if (window.ActiveXObject)
                xObj = new ActiveXObject("Microsoft.XMLHTTP");

            if (xObj) {
                xObj.open("GET", '/GetAjaxSession.aspx?QT=U3UJT');
                xObj.onreadystatechange = function () {
                    if (xObj.readyState == 4 && xObj.status == 200 && xObj.responseXML != null) {
                        if (xObj.responseXML.getElementsByTagName("OTP").length > 0) {
                            document.getElementById("spnerrMsg").innerHTML = xObj.responseXML.getElementsByTagName("OTP")[0].firstChild.data;
                            rid = xObj.responseXML.getElementsByTagName("Request_ID")[0].firstChild.data;
                            if (rid == '0') {
                                jQueryMsg('Secure Zone!', 'OTP not required', 2);
                            }
                            else {
                                document.getElementById("Footer1_txtOTP").style.display = "";
                                document.getElementById("Footer1_btnOTPValidate").style.display = "";
                                document.getElementById("Footer1_btnGetOTP").style.display = "none";
                            }
                        }
                    }
                }
                xObj.send(null);
            }
        }
        function ValidatedOTP() {
            document.getElementById("spnerrMsg").innerHTML = "";

            if (document.getElementById("Footer1_txtOTP").value == "") {
                document.getElementById("spnerrMsg").innerHTML = "Enter OTP!";
                jQueryMsg('Enter OTP!', 'Please enter OTP');
                return false;
            }

            var xObj = false;
            if (window.XMLHttpRequest)
                xObj = new XMLHttpRequest();
            else if (window.ActiveXObject)
                xObj = new ActiveXObject("Microsoft.XMLHTTP");

            if (xObj) {
                xObj.open("GET", '/GetAjaxSession.aspx?QT=B4MKY&OTP=' + document.getElementById("Footer1_txtOTP").value + '&RID=' + rid);
                xObj.onreadystatechange = function () {
                    if (xObj.readyState == 4 && xObj.status == 200 && xObj.responseXML != null) {
                        if (xObj.responseXML.getElementsByTagName("IsValid").length > 0) {
                            if (xObj.responseXML.getElementsByTagName("IsValid")[0].firstChild.data == "true") {
                                jQueryMsg('Success', 'Logged In Successfully!', 2);
                                document.getElementById("spnerrMsg").innerHTML = "<span class='textgreen'>Logged In Successfully!</span>";
                            }
                            else {
                                document.getElementById("spnerrMsg").innerHTML = "Invalid OTP!";
                                jQueryMsg('Invalid OTP!', 'OTP entered is incorrect');
                            }
                        }
                    }
                }
                xObj.send(null);
            }
        }
